 namespace Microsoft.Quantum.Testing.QIR { open Microsoft.Quantum.Intrinsic; @EntryPoint() operation QuantumRandomNumberGenerator() : Int { mutable randomNumber = 0; for i in 1 .. 64 { use q = Qubit(); H(q); set randomNumber = M(q); if M(q) == One { set randomNumber += 1; } } return randomNumber; } }